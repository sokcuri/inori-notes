import { Entity, BaseEntity, PrimaryColumn, Column } from 'typeorm';
import { ObjectType, Field, ID, Int, Float } from 'type-graphql';

@ObjectType()
@Entity({ name: '{{name}}' })
export class {{className}} extends BaseEntity {
  {{#each classFields}}
  {{{field}}}
  {{{column}}}
  {{{camelName}}}: {{{type}}};

  {{/each}}
  {{#each mergeFields}}
  @Field(type => [{{{type}}}])
  get {{{name}}}() {
    return [{{{fieldText}}}];
  }

  {{/each}}
}

